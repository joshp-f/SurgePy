{
	"name": "surge",
	"displayName": "Surge",
	"publisher": "joshp-f",
	"description": "Create type definitions from programs run history and read previous values",
	"repository": "https://github.com/joshp-f/SurgeVSCode",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.56",
		"@types/vscode": "^1.30.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"tslint": "^5.20.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"slash": "^3.0.0"
	}
}
